	month = aug,
	year = {2022},
	pages = {2043--2055},
	file = {Hurme et al_2022_Fruit bat migration.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Hurme et al_2022_Fruit bat migration.pdf:application/pdf},

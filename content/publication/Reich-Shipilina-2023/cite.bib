	note = {Publisher: Elsevier Ireland Ltd},
	keywords = {Cleaning protocol, Diagenesis, Forensic taphonomy, Human scalp hair, Isotopic analysis, Postmortem degradation},
	pages = {110463},
	file = {Kootker et al_2020_Reprint of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Kootker et al_2020_Reprint of.pdf:application/pdf},
}


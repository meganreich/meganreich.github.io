	author = {Ramaswamy, Sonny B. and Shu, Shengqiang and Park, Yong Ihl and Zeng, Fanrong},
	year = {1997},
	keywords = {Moth, Butterfly, Ecdysteroid, Egg maturation, Monandry, Oogenesis, Polyandry, Vitellogenesis},
	pages = {539--558},
	file = {Ramaswamy et al_1997_Dynamics of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Ramaswamy et al_1997_Dynamics of.pdf:application/pdf},
}


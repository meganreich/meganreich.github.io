	year = {2020},
	keywords = {biostatistics, correlated predictors, distributional assumptions, linear mixed-effects models, missing random effects, statistical quantification of individual differenc},
	pages = {1141--1152},
	file = {Schielzeth et al_2020_Robustness of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Schielzeth et al_2020_Robustness of.pdf:application/pdf},

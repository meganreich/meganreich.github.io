	journal = {PNAS},
	author = {Lister, Bradford C and Garcia, Andres},
	year = {2018},
	pages = {E10397--E10406},
	file = {Lister_Garcia_2018_Climate-driven.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Lister_Garcia_2018_Climate-driven.pdf:application/pdf},
}


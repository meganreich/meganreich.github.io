	journal = {BMC Ecology and Evolution},
	author = {Glück, Marcel and Geue, Julia C. and Thomassen, Henri A.},
	month = dec,
	year = {2022},
	pages = {8},
	file = {Glück et al_2022_Environmental.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Glück et al_2022_Environmental.pdf:application/pdf},
}


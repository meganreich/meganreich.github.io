@article{Flockhart2017c,
	title = {Regional climate on the breeding grounds predicts variation in the natal origin of monarch butterflies overwintering in {Mexico} over 38 years},
	volume = {23},
	issn = {13652486},
	url = {http://doi.wiley.com/10.1111/gcb.13589},
	doi = {10.1111/gcb.13589},
	abstract = {Addressing population declines of migratory insects requires linking populations across different portions of the annual cycle and understanding the effects of variation in weather and climate on productivity, recruitment, and patterns of long-distance movement. We used stable H and C isotopes and geospatial modeling to estimate the natal origin of monarch butterflies (Danaus plexippus) in eastern North America using over 1000 monarchs collected over almost four decades at Mexican overwintering colonies. Multinomial regression was used to ascertain which climate-related factors best-predicted temporal variation in natal origin across six breeding regions. The region producing the largest proportion of overwintering monarchs was the US Midwest (mean annual proportion = 0.38; 95\% CI: 0.36–0.41) followed by the north-central (0.17; 0.14–0.18), northeast (0.15; 0.11–0.16), northwest (0.12; 0.12–0.16), southwest (0.11; 0.08–0.12), and southeast (0.08; 0.07–0.11) regions. There was no evidence of directional shifts in the relative contributions of different natal regions over time, which suggests these regions are comprising the same relative proportion of the overwintering population in recent years as in the mid-1970s. Instead, interannual variation in the proportion of monarchs from each region covaried with climate, as measured by the Southern Oscillation Index and regional-specific daily maximum temperature and precipitation, which together likely dictate larval development rates and food plant condition. Our results provide the first robust long-term analysis of predictors of the natal origins of monarchs overwintering in Mexico. Conservation efforts on the breeding grounds focused on the Midwest region will likely have the greatest benefit to eastern North American migratory monarchs, but the population will likely remain sensitive to regional and stochastic weather patterns.},
	number = {7},
	urldate = {2017-11-30},
	journal = {Global Change Biology},
	author = {Flockhart, D.T.Tyler and Brower, Lincoln P. and Ramirez, M. Isabel and Hobson, Keith A. and Wassenaar, Leonard I. and Altizer, Sonia and Norris, D. Ryan},
	month = jul,
	year = {2017},
	note = {Publisher: Wiley/Blackwell (10.1111)},
	keywords = {Danaus plexippus, stable isotopes, provenance, Asclepias, seasonal migration, migratory connectivity, hydrogen, carbon},
	pages = {2565--2576},
	file = {Flockhart et al_2017_Regional climate on.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Flockhart et al_2017_Regional climate on.pdf:application/pdf;Flockhart et al_2017_Regional climate on.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Flockhart et al_2017_Regional climate on2.pdf:application/pdf},
}

@article{Malcolm1993b,
	title = {Spring recolonization of eastern {North} {America} by the monarch butterfly: successive brood or single sweep migration?},
	abstract = {Each March, Danaus plexippus remigrate north from Mexican overwintering sites. By June they are distributed across most of the E North America, north to S Canada. The authors distinguish between two alternative spring recolonization strategies: 1) "single sweep recolonization', in which overwintered individual remigrants from Mexico were hypothesized to colonize the entire eastern North American breeding range, and 2) "successive brood recolonization' in which remigrants from Mexico were hypothesized to lay all of their eggs in the S USA and then die, leaving their offspring to continue the migration northward and recolonize the northern breeding range. Cardenolide concentrations, cardenolide "finger prints;, and wind wear of migrant butterflies captured along two latitudinal transects in the spring of 1985 provided three lines of independent support consistent with hypothesis 2. -from Authors},
	number = {March 2016},

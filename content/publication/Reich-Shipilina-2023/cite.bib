	journal = {Journal of Applied Ecology},
	author = {Sánchez-Zapata, José A. and Donázar, José A. and Delgado, Antonio and {Antonio Delgado} and Forero, Manuela G. and Ceballos, Olga and Hiraldo, Fernando},

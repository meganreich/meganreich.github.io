	file = {Barroso_Dutheil_2023_The landscape of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Barroso_Dutheil_2023_The landscape of.pdf:application/pdf},
}

@article{talavera_afrotropical_2023,
	title = {The {Afrotropical} breeding grounds of the {Palearctic}-{African} migratory painted lady butterflies ( \textit{{Vanessa} cardui} )},
	volume = {120},

	pages = {69--78},
	file = {Lorenz et al_2013_Does Autocthonous.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Lorenz et al_2013_Does Autocthonous.pdf:application/pdf},

	year = {2014},
	pmid = {24118574},
	note = {ISBN: 09621083 (ISSN)},
	keywords = {16S rRNA sequencing, convergence, gut microbiome, mammals, microbial diversity},
	pages = {1301--1317},
	file = {Delsuc et al_2014_Convergence of gut.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Delsuc et al_2014_Convergence of gut.pdf:application/pdf},
}


@article{Terenius2011,
	title = {{RNA} interference in {Lepidoptera}: {An} overview of successful and unsuccessful studies and implications for experimental design},
	volume = {57},
	issn = {00221910},
	doi = {10.1016/j.jinsphys.2010.11.006},
	abstract = {Gene silencing through RNA interference (RNAi) has revolutionized the study of gene function, particularly in non-model insects. However, in Lepidoptera (moths and butterflies) RNAi has many times proven to be difficult to achieve. Most of the negative results have been anecdotal and the positive experiments have not been collected in such a way that they are possible to analyze. In this review, we have collected detailed data from more than 150 experiments including all to date published and many unpublished experiments. Despite a large variation in the data, trends that are found are that RNAi is particularly successful in the family Saturniidae and in genes involved in immunity. On the contrary, gene expression in epidermal tissues seems to be most difficult to silence. In addition, gene silencing by feeding dsRNA requires high concentrations for success. Possible causes for the variability of success in RNAi experiments in Lepidoptera are discussed. The review also points to a need to further investigate the mechanism of RNAi in lepidopteran insects and its possible connection to the innate immune response. Our general understanding of RNAi in Lepidoptera will be further aided in the future as our public database at http://insectacentral.org/RNAi will continue to gather information on RNAi experiments. © 2010 Elsevier Ltd.},
	number = {2},
	journal = {Journal of Insect Physiology},
	author = {Terenius, Olle and Papanicolaou, Alexie and Garbutt, Jennie S. and Eleftherianos, Ioannis and Huvenne, Hanneke and Kanginakudru, Sriramana and Albrechtsen, Merete and An, Chunju and Aymeric, Jean Luc and Barthel, Andrea and Bebas, Piotr and Bitra, Kavita and Bravo, Alejandra and Chevalier, François and Collinge, Derek P. and Crava, Cristina M. and de Maagd, Ruud A. and Duvic, Bernard and Erlandson, Martin and Faye, Ingrid and Felföldi, Gabriella and Fujiwara, Haruhiko and Futahashi, Ryo and Gandhe, Archana S. and Gatehouse, Heather S. and Gatehouse, Laurence N. and Giebultowicz, Jadwiga M. and Gómez, Isabel and Grimmelikhuijzen, Cornelis J.P. and Groot, Astrid T. and Hauser, Frank and Heckel, David G. and Hegedus, Dwayne D. and Hrycaj, Steven and Huang, Lihua and Hull, J. Joe and Iatrou, Kostas and Iga, Masatoshi and Kanost, Michael R. and Kotwica, Joanna and Li, Changyou and Li, Jianghong and Liu, Jisheng and Lundmark, Magnus and Matsumoto, Shogo and Meyering-Vos, Martina and Millichap, Peter J. and Monteiro, Antónia and Mrinal, Nirotpal and Niimi, Teruyuki and Nowara, Daniela and Ohnishi, Atsushi and Oostra, Vicencio and Ozaki, Katsuhisa and Papakonstantinou, Maria and Popadic, Aleksandar and Rajam, Manchikatla V. and Saenko, Suzanne and Simpson, Robert M. and Soberón, Mario and Strand, Michael R. and Tomita, Shuichiro and Toprak, Umut and Wang, Ping and Wee, Choon Wei and Whyard, Steven and Zhang, Wenqing and Nagaraju, Javaregowda and ffrench-Constant, Richard H. and Herrero, Salvador and Gordon, Karl and Swevers, Luc and Smagghe, Guy},

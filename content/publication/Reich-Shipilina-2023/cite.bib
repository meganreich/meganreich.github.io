	pages = {1--21},
	file = {Wepprichid et al_2019_Butterfly abundance.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Wepprichid et al_2019_Butterfly abundance.pdf:application/pdf},

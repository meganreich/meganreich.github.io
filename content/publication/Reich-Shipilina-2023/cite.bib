	pages = {4348--4370},
	file = {Rellstab et al_2015_A practical guide.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Rellstab et al_2015_A practical guide.pdf:application/pdf},
}


	author = {Jin Long, Ma and Wei, Gang Jian and Liu, Ying and Ren, Zhong Yuan and Xu, Yi Gang and Yang, Yong Hong},
	year = {2013},
	keywords = {87Sr/86Sr, geological standards, IAPSO, MC-ICP-MS, stable Sr isotopes, δ88/86Sr},
	pages = {3111--3118},

	file = {Delmore_Liedvogel_2016_Investigating.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Delmore_Liedvogel_2016_Investigating.pdf:application/pdf},
}

@article{Parker1985,

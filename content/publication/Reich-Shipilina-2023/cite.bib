@article{Ramenofsky2007,
	title = {Regulation of migration},
	volume = {57},
	issn = {00063568},
	doi = {10.1641/B570208},
	abstract = {Migration is a widespread and ancient phenomenon commonly involving a seasonal response to predictable changes in the environment. Such changes include the four seasons at the higher latitudes and wet-dry seasons in the tropics. In general, migrations are movements to breeding grounds followed by a postbreeding return to areas for nonreproductive activities. We focus on these seasonal migrations and summarize processes by which diverse organisms prepare and adjust to different phases of the migration life history stage, such as preparation, onset (actual traveling), and termination. This framework enables investigations of physiological and behavioral mechanisms involved in each phase, as well as studies of how environmental signals control this diverse and successful process across the taxa. © 2007 American Institute of Biological Sciences.},
	number = {2},
	journal = {BioScience},
	author = {Ramenofsky, Marilyn and Wingfield, John C.},
	year = {2007},

	note = {Publisher: Wiley/Blackwell (10.1111)
ISBN: 1752-4598},
	keywords = {★, Lepidoptera, Migration, migration, Conservation, habitat, monitoring, endangered biological phenomenon, Endangered biological phenomenon, Habitat, Monitoring},
	pages = {95--100},
	file = {Brower et al_2012_Decline of monarch.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Brower et al_2012_Decline of monarch.pdf:application/pdf},

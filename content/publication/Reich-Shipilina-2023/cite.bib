	pages = {897--910},
	file = {Reim et al_2019_Nutritional stress.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Reim et al_2019_Nutritional stress.pdf:application/pdf},

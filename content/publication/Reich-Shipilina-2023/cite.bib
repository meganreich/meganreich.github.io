	file = {J of Evolutionary Biology - 2014 - Mattila - Heritability of flight and resting metabolic rates in the Glanville fritillary.pdf:C\:\\Users\\megsr\\Downloads\\J of Evolutionary Biology - 2014 - Mattila - Heritability of flight and resting metabolic rates in the Glanville fritillary.pdf:application/pdf},

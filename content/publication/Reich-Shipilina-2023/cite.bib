	note = {Publisher: Parasites \& Vectors
ISBN: 1307101720},
	keywords = {Parasites, Bioindication, Biomarker, Metal pollution, Ecosystem health, Ecotoxicology, Endocrine disruption},
	pages = {1--19},
	file = {Sures et al_2017_Parasite responses.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Sures et al_2017_Parasite responses.pdf:application/pdf},
}


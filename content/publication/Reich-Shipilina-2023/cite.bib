@article{guerra_monarch_2020,
	title = {The {Monarch} {Butterfly} as a {Model} for {Understanding} the {Role} of {Environmental} {Sensory} {Cues} in {Long}-{Distance} {Migratory} {Phenomena}},
	volume = {14},

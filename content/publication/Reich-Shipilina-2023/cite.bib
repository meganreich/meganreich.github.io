	pages = {253--262},
	file = {Diffendorfer et al_2014_National Valuation.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Diffendorfer et al_2014_National Valuation.pdf:text/html;Diffendorfer et al_2014_National Valuation.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Diffendorfer et al_2014_National Valuation2.pdf:application/pdf;PDF:C\:\\Users\\megsr\\Zotero\\storage\\PLYSTTL9\\Diffendorfer-2014-National-valuation-of-monarch-butte.pdf:application/pdf},

	pages = {1--9},
	file = {Wilcox et al_2021_Captive-reared.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Wilcox et al_2021_Captive-reared.pdf:application/pdf},
}

@article{Grzes2010,

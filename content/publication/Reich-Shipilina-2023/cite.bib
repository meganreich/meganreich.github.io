	pages = {115--126},
	file = {Hekkala et al_2021_Ecological.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Hekkala et al_2021_Ecological.pdf:application/pdf},

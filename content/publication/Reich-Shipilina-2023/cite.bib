	year = {1999},
	pages = {1--20},
	file = {Glessmer et al_1999_Taking ownership of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Glessmer et al_1999_Taking ownership of.pdf:application/pdf},

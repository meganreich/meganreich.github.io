	year = {2019},
	note = {Publisher: International Journal of Tropical Insect Science},
	keywords = {Insect pests, Bacterial community, Biological barcoding, Geo-location fingerprinting, Helicoverpa armigera, Migrant insects, PCR-DGGE},

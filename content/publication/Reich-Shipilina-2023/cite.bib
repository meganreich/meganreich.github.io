@article{DeRoode2005,
	title = {Virulence and competitive ability in genetically diverse malaria infections},
	volume = {102},
	issn = {00278424},
	doi = {10.1073/pnas.0500078102},
	abstract = {Explaining parasite virulence is a great challenge for evolutionary biology. Intuitively, parasites that depend on their hosts for their survival should be benign to their hosts, yet many parasites cause harm. One explanation for this is that within-host competition favors virulence, with more virulent strains having a competitive advantage in genetically diverse infections. This idea, which is well supported in theory, remains untested empirically. Here we provide evidence that within-host competition does indeed select for high parasite virulence. We examine the rodent malaria Plasmodium chabaudi in laboratory mice, a parasite-host system in which virulence can be easily monitored and competing strains quantified by using strain-specific real-time PCR. As predicted, we found a strong relationship between parasite virulence and competitive ability, so that more virulent strains have a competitive advantage in mixed-strain infections. In transmission experiments, we found that the strain composition of the parasite populations in mosquitoes was directly correlated with the composition of the blood-stage parasite population. Thus, the outcome of within-host competition determined relative transmission success. Our results imply that within-host competition is a major factor driving the evolution of virulence and can explain why many parasites harm their hosts. © 2005 by The National Academy of Sciences of the USA.},
	number = {21},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {De Roode, Jacobus C. and Pansini, Riccardo and Cheesman, Sandra J. and Helinski, Michelle E.H. and Huijben, Silvie and Wargo, Andrew R. and Bell, Andrew S. and Chan, Brian H.K. and Walliker, David and Read, Andrew F.},
	year = {2005},
	pmid = {15894623},
	keywords = {Competition, Parasite, Evolution, Mixed infection, Plasmodium},
	pages = {7624--7628},
	file = {De Roode et al_2005_Virulence and.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\De Roode et al_2005_Virulence and.pdf:application/pdf},
}

@article{Wooller2021,
	title = {Lifetime mobility of an {Arctic} woolly mammoth},
	volume = {373},
	issn = {10959203},
	doi = {10.1126/science.abg1134},
	abstract = {Little is known about woolly mammoth (Mammuthus primigenius) mobility and range. Here we use high temporal resolution sequential analyses of strontium isotope ratios along an entire 1.7-meter-long tusk to reconstruct the movements of an Arctic woolly mammoth that lived 17,100 years ago, during the last ice age. We use an isotope-guided random walk approach to compare the tusk's strontium and oxygen isotope profiles to isotopic maps. Our modeling reveals patterns of movement across a geographically extensive range during the animal's {\textasciitilde}28-year life span that varied with life stages. Maintenance of this level of mobility by megafaunal species such as mammoth would have been increasingly difficult as the ice age ended and the environment changed at high latitudes.},
	number = {6556},
	journal = {Science},
	author = {Wooller, Matthew J. and Bataille, Clement and Druckenmiller, Patrick and Erickson, Gregory M. and Groves, Pamela and Haubenstock, Norma and Howe, Timothy and Irrgeher, Johanna and Mann, Daniel and Moon, Katherine and Potter, Ben A. and Prohaska, Thomas and Rasic, Jeffrey and Reuther, Joshua and Shapiro, Beth and Spaleta, Karen J. and Willis, Amy D.},
	year = {2021},

	journal = {Ecological Applications},
	author = {Comay, Orr and Ben Yehuda, Oz and Benyamini, Dubi and Schwartz‐Tzachor, Racheli and Pe'er, Israel and Melochna, Tal and Pe’er, Guy},
	month = dec,

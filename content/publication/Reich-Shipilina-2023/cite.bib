@article{Beetz2021,
	title = {Flight-{Induced} {Compass} {Representation} in the {Monarch} {Butterfly} {Heading} {Network}},
	doi = {10.2139/ssrn.3908784},
	abstract = {23 For navigation, animals use a robust internal compass. Compass navigation is especially crucial for long-24 distance migrating animals like monarch butterflies, which use a sun compass to navigate every fall over 25 4,000 km to their overwintering sites. The central complex, a brain region equipped with sun-compass 26 neurons, is proposed to control the butterfly's heading. Although the activity of central-complex neurons 27 exhibits a locomotor-dependent modulation in many insects, the consequences of such modulations for 28 the coding of heading remain unexplored. Here, we developed tetrode recordings from tethered flying 29 monarch butterflies to reveal how flight modulates heading representation. We found that during flight, 30 heading-direction neurons change their tuning, transforming the central-complex network to function as 31 a global compass. This compass coding is characterized by the dominance of steering feedback and 32 allows for robust heading representation even under unreliable visual scenarios, an ideal strategy for 33 maintaining a migratory heading over enormous distances. 34},

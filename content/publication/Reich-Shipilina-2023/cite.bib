	keywords = {http://dx.doi.org/10.1029/2010JG001581, doi:10.102},
	file = {Bowen et al_2011_Water balance model.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Bowen et al_2011_Water balance model.pdf:application/pdf;PDF:C\:\\Users\\megsr\\Zotero\\storage\\UHRPY7EC\\Bowen_et_al-2011-Journal_of_Geophysical_Research__Biogeosciences_(2005-2012).pdf:application/pdf},

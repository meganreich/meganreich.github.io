}

@article{Cerreta2022,
	title = {Comparative nutrient analysis of four species of cockroaches used as food for insectivores by life stage, species, and sex},
	volume = {41},
	issn = {10982361},
	doi = {10.1002/zoo.21647},
	abstract = {A variety of insects are fed to insectivorous animals; however, nutritional analyses are often limited to adult life stages. Four species of nymph and adult female and male cockroaches (Blaberus giganteus, Blaptica dubia, Blatta lateralis, and Gromphadorhina portentosa) were analyzed for moisture, crude protein (CP), acid detergent fiber (ADF), fat, ash, and mineral content. The small sample size of this study precluded statistical analyses, however comparatively, CP in adult B. lateralis and B. dubia was greater than the CP in nymphs of the same species. Adult and nymph B. dubia had the greatest CP (96.6\% and 65.3\%, respectively) compared to the other three species. Inversely, fat content in adult B. lateralis and B. dubia was lower than that of nymphs of the same species. All adults contained similar levels of ADF, yet adult B. giganteus had greater ADF than nymphs; conversely, B. lateralis and B. dubia adults had less ADF than nymphs. There were differences noted in mineral parameters among the four species and life stages. Adult B. giganteus had less Ca than G. portentosa, and the lowest Ca content of the four species of nymphs and adults analyzed. This study underscores the differences in nutrient content with respect to life stage and species in previously understudied cockroaches to improve nutrient intake in captive insectivores.},

	author = {Rodiouchkina, Katerina and Rodushkin, Ilia and Goderis, Steven and Vanhaecke, Frank},
	year = {2022},
	pmid = {34863743},
	note = {Publisher: Elsevier B.V.},
	keywords = {Hair, Fractionation, Isotope ratio, Multi-collector ICP-Mass Spectrometry (MC-ICP-MS), Nails, Trace element concentration},
	pages = {152059},
	file = {Rodiouchkina et al_2022_Longitudinal.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Rodiouchkina et al_2022_Longitudinal.pdf:application/pdf},
}

@article{Peijnenburg2003,

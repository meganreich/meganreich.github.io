	author = {Hobbie, Erik A. and Ouimette, Andrew P.},
	year = {2009},
	keywords = {Modeling, Denitrification, Isotopic fractionation, Mycorrhizal fungi, Nitrogen isotopes, Soil horizons, Soil mixing},
	pages = {355--371},
	file = {Hobbie_Ouimette_2009_Controls of.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Hobbie_Ouimette_2009_Controls of.pdf:application/pdf},
}

@article{Cernusak2016,

	keywords = {heavy metals, Black soldier fly larvae, contaminants accumulation, feed and food safety, mycotoxins, pesticides, rearing, substrate contamination},
	pages = {1410--1420},
	file = {Purschke et al_2017_Impact of substrate.pdf:C\:\\Users\\megsr\\OneDrive - University of Ottawa\\Documents\\Zotero\\Purschke et al_2017_Impact of substrate.pdf:application/pdf},
